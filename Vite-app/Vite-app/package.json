{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build:dev": "vite build --mode development",
    "build:prod": "tsc -b && vite build",
    "clean": "rmdir /s /q dist node_modules\\.cache 2>nul",
    "cold:dev": "rmdir /s /q dist node_modules\\.cache 2>nul & npm run build:dev",
    "cold:prod": "rmdir /s /q dist node_modules\\.cache 2>nul & npm run build:prod",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/react-router": "^5.1.20",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.11.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
